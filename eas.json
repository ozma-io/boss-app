{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "prebuildCommand": "npm run fix-android-manifest"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "preview",
      "android": {
        "prebuildCommand": "npm run fix-android-manifest"
      }
    },
    "preview-device": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "prebuildCommand": "npm run fix-android-manifest"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "android": {
        "prebuildCommand": "npm run fix-android-manifest"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6755306941"
      },
      "android": {
        "track": "production",
        "releaseStatus": "draft"
      }
    },
    "internal": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}

