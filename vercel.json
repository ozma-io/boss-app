{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "devCommand": "npm run web",
  "installCommand": "npm install",
  "framework": null,
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- 'app/**' 'components/**' 'services/**' 'hooks/**' 'contexts/**' 'utils/**' 'types/**' 'constants/**' 'assets/**' 'public/**' 'index.js'",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

